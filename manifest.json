{
  "manifest_version": 2,
  "name": "humblextractor",
  "version": "1.0",
  "description": "Extracts game keys from Humble Bundle.",
  "permissions": [
    "activeTab",
    "storage",
    "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "48": "icons/icon.png"
    },
    "default_title": "humblextractor menu",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icons/icon.png"
  }
}

